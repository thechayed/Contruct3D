{
    "manifest_version": 3,
    "name": "Construct3D",
    "version": "0.1.0",
    "description": "Adds a proper 3D Editor Viewport to Construct 3 using Three.js.",
    "permissions": [
      "activeTab"
    ],
    "background": {
    },
    "action": {
      "default_popup": "./popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["C3D_Editor_Webpack.js"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["overlay.css"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  